<h3><%= tableName %> 자료 목록</h3>
<table
  class="table table-bordered border-primary table-hover"
  style="text-align: center"
>
  <thead class="table-primary">
    <tr>
      <% columns.forEach(function(col) { %>
      <th><%= col.COLUMN_COMMENT || col.COLUMN_NAME %></th>
      <% }); %>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% if (dataRows && dataRows.length > 0) { %> <%
    dataRows.forEach(function(row) { %>
    <tr>
      <% columns.forEach(function(col) { %>
      <td><%= row[col.COLUMN_NAME] %></td>
      <% }); %>
      <td>생성</td>
      <td>수정</td>
      <td>삭제</td>
    </tr>
    <% }); %> <% } else { %>
    <tr>
      <td colspan="<%= columns.length + 3 %>">자료가 없습니다.</td>
    </tr>
    <% } %>
  </tbody>
</table>
